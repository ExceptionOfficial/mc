{
    "multipart": [
        {   "apply": { "model": "spruce_fence_post" }},
        {   "when": { "north": "true" },
            "apply": [
			{ "model": "spruce_fence_side", "uvlock": true },
			{ "model": "spruce_fence_side_0", "uvlock": true },
			{ "model": "spruce_fence_side_1", "uvlock": true },
			{ "model": "spruce_fence_side_2", "uvlock": true },
			{ "model": "spruce_fence_side_3", "uvlock": true }
        ]},
        {   "when": { "east": "true" },
            "apply": [
			{ "model": "spruce_fence_side", "y": 90, "uvlock": true },
			{ "model": "spruce_fence_side_0", "y": 90, "uvlock": true },
			{ "model": "spruce_fence_side_1", "y": 90, "uvlock": true },
			{ "model": "spruce_fence_side_2", "y": 90, "uvlock": true },
			{ "model": "spruce_fence_side_3", "y": 90, "uvlock": true }
        ]},
        {   "when": { "south": "true" },
            "apply": [
			{ "model": "spruce_fence_side", "y": 180, "uvlock": true },
			{ "model": "spruce_fence_side_0", "y": 180, "uvlock": true },
			{ "model": "spruce_fence_side_1", "y": 180, "uvlock": true },
			{ "model": "spruce_fence_side_2", "y": 180, "uvlock": true },
			{ "model": "spruce_fence_side_3", "y": 180, "uvlock": true }
        ]},
        {   "when": { "west": "true" },
            "apply": [
			{ "model": "spruce_fence_side", "y": 270, "uvlock": true },
			{ "model": "spruce_fence_side_0", "y": 270, "uvlock": true },
			{ "model": "spruce_fence_side_1", "y": 270, "uvlock": true },
			{ "model": "spruce_fence_side_2", "y": 270, "uvlock": true },
			{ "model": "spruce_fence_side_3", "y": 270, "uvlock": true }
        ]}
    ]
}
